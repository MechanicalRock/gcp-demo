<script type="text/javascript">
	function submitEnquiry() {
		const enquiryForm = document.querySelector( '.enquiry-form' );

		if ( enquiryForm ) {
			const email = enquiryForm.querySelector( '[name=email]' );
			const message = enquiryForm.querySelector( '[name=message]' );

			if ( email && message ) {
				const emailVal = email.value.trim();
				const messageVal = message.value.trim();
				const submitButton = document.querySelector( '.submit-btn' )

				if ( emailVal.length > 0 && messageVal.length > 0 ) {
					const submitIcon = submitButton.querySelector( '.fa' );
					submitIcon.classList.remove( 'fa-thumbs-up' );
					submitIcon.classList.add( 'fa-spinner' );
					enquiryForm.classList.add( '-posting' );
					submitButton.disabled = true;

					const resetUI = () => {
						submitIcon.classList.remove( 'fa-spinner' );
						submitIcon.classList.add( 'fa-thumbs-up' );
						enquiryForm.classList.remove( '-posting' );
						submitButton.disabled = false;
					}

					const headers = new Headers();
					// headers.append( "Accept", "application/json" );
					headers.append( "content-type", "application/x-www-form-urlencoded" );
					headers.append( "X-Content-Type-Options", "nosniff" );

					const formBody = new FormData();
					formBody.append( 'email', emailVal );
					formBody.append( 'message', messageVal );

					fetch( 'https://asia-northeast1-central-oath-228105.cloudfunctions.net/http', {
						method: 'POST',
						body: `email=${emailVal}&message=${messageVal}`,
						mode: "no-cors",
						cache: "no-cache",
						headers
					} ).then( ( response ) => {
					} ).then( ( data ) => {
						resetUI();
						enquiryForm.classList.add( '-collapse' );
					} ).catch( ( reason ) => {
						resetUI();
						submitIcon.classList.remove( 'fa-thumbs-up' );
						submitIcon.classList.add( 'fa-thumbs-down' );
						submitButton.classList.add( '-error' );
					} );
				}
			}
		}
	}
</script>
